pipeline {
    agent any
    tools { 
        maven 'maven3.6.3' 
        jdk 'jdk' 
    }
	
    stages {
        
		stage ('Initialize') {
            steps {
                sh '''
                    echo "PATH = ${PATH}"
                    echo "M2_HOME = ${M2_HOME}"
                ''' 
            }
        }
		
		stage ('static code analysis') {
           steps {
                sh 'mvn validate' 
            }
		}
		
		stage ('Build') {
           steps {
                sh 'mvn compile' 
            }
		}
		
		stage ('Package') {
           steps {
                sh 'mvn package' 
            }
		}
		
		stage ('Run UI Tests') {
           steps {
                sh 'mvn -f functionaltest/pom.xml  test' 
            }
		}
		
		stage ('Package') {
           steps {
                sh 'mvn package' 
            }
		}
	}
}


